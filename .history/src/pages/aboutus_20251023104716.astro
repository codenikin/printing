---
import Layout from "../layouts/Layout.astro";
import { Swiper, SwiperWrapper, SwiperSlide } from "astro-swiper";
const testimonials = [
  {
    id: 1,
    image: "/assets/img/testimonial/01.jpg",
    quote:
      "We ordered custom T-shirts for our company event, and Ommuruga Printing Service delivered them exactly on time. The print quality was sharp, the colors were perfect, and the fabric felt premium. Highly recommended for anyone in Hosur looking for professional printing work.",
    name: "Priya Rajesh, Anna Nagar, Hosur",
    position: "HR Manager DELCO Pvt Ltd",
  },
  {
    id: 2,
    image: "/assets/img/testimonial/01.jpg",
    quote:
      "We’ve been using Ommuruga Printing Service for all our office printing needs — brochures, flyers, and banners. The offset printing quality is top-notch, and they always meet our deadlines. Their prices are also very reasonable for the quality they provid.",
    name: "Meena Lakshmi, Bagalore",
    position: "Marketing Head Tech Solutions",
  },
  {
    id: 3,
    image: "/assets/img/testimonial/01.jpg",
    quote:
      "The team really listened to my ideas and turned them into a perfect wedding invitation design. Everyone loved the print and the paper quality. I’m so happy with their creativity and dedication. Thank you, Ommuruga team.",
    name: "Suresh R., Industrial Area, Hosur",
    position: "CEO & Founder Hitech Industry",
  },
];
---

<Layout>
  <div
    class="breadcrumb-wrapper section-padding bg-cover"
    style="background-image: url('/assets/img/bg.png');"
  >
    <div class="container">
      <div class="page-heading">
        <div class="breadcrumb-sub-title text-center">
          <h1
            class="wow fadeInUp text-white"
            data-aos="fade-up"
            data-aos-anchor-placement="top-bottom"
            data-aos-duration="1500"
          >
            About Us
          </h1>
          <ul class="breadcrumb-items wow fadeInUp" data-wow-delay=".5s">
            <li>
              <a href="index.html"> Home </a>
            </li>
            <li>
              <i class="fas fa-minus"></i>
            </li>
            <li>About Us</li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <section class="about-section section-padding">
    <div class="container">
      <div class="about-wrapper">
        <div class="row g-4">
          <div class="col-lg-6">
            <div class="about-content">
              <div class="section-title">
                <div class="sub-title wow fadeInUp">
                  <span>More about us</span>
                </div>
                <h2 class="split-text right">
                  Professional Printing Experts in Hosur
                </h2>
              </div>
              <p class="mt-3 mt-md-0 wow fadeInUp" data-wow-delay=".5s">
                Ommuruga Printing Services is a one-stop printing solution in
                Hosur offering creative, reliable, and affordable print
                services. With years of experience and advanced printing
                technology, we bring your ideas to life — from business
                marketing materials to wedding and event signage. Our goal is to
                deliver top-notch quality, on time, every time
              </p>
              <div class="icon-box-items">
                <div class="icon-items wow fadeInUp" data-wow-delay=".3s">
                  <div class="icons">
